<%if @events %>
<h1>Events#index</h1>
<p>DASHBOARD/EVENTS</p>



  <table>
    <thead>
      <tr>
        <th>Title </th> 
        <th>Date </th>
        <th>Time </th>
        <th>Tagline </th>
        <th>Address </th>
        <th>Suite() </th>
        <th>City </th>
        <th>State </th>
        <th>Zip </th>
        <th>More Info </th>
        <th>Photo </th>
        <th>Photo </th>
        <th>Photo </th>
        <th>Website </th>
        <th>Facebook </th>
        <th>Application Form </th>
        <th>Contact Name </th>
        <th>Contact Email </th>
        <th></th>
        <th></th>
        <th></th>

      </tr> 
    </thead>

    <tbody>
      <% @events.each do |event| %>
          <tr>  
            <td><%= event.title %></td>

            <td><%= event.date %></td>
            <td><%= event.time %></td>
            <td><%= event.tagline %></td>
            <td><%= event.address %></td>
            <td><%= event.address2 %></td>
            <td><%= event.city %></td>
            <td><%= event.state %></td>
            <td><%= event.zip %></td>
            <td><%= event.more_info %></td>
            <td><%= event.photo1 %></td>
            <td><%= event.photo2 %></td>
