<div class="event_show_page">
  <div  class="col-md-5 pull-left event_show">
    <h3><%= @profile.name %></h3>
    <p>
    <em><%= @profile.tagline %></em>
    </p>
    <p id="avatar">
    <strong>Avatar:</strong>
    <%= image_tag(@profile.avatar.url(:medium)) %>
    </p>
    <br>
    <div class="small_font">
      <p>
      <%= @profile.bio %>
      </p>
    </div>
    <br>
    <p>
    Website:
    <%= @profile.website1 %>
    </p>
    <p>
    Facebook:
    <%= @profile.facebook %>
    </p>
    <p>
    Twitter:
    <%= @profile.twitter %>
    </p>
  </div>
  <div class="pull-left">
    <p></p><br>
    <div class="bigger_box">
      <div class="green_box">
      </div>
    </div>
  </div>
</div>
<p>Upload Art Photo:</p>
<%= form_for([:dashboard, @profile, @art_photo]) do |f| %>
<%= f.file_field :photo %>
<%= f.submit('Upload Image', class: 'btn btn-primary') %>
<% end %>
<h5>
<%= link_to 'Edit', edit_dashboard_profile_path(@profile) %> |
<%= link_to 'Back', dashboard_profiles_path %></h5>
