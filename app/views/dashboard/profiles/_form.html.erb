<%= form_for([:dashboard, @profile], multipart: true ) do |f| %>
<% if @profile.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(@profile.errors.count, "error") %> prohibited this profile from being saved:</h2>
  <ul>
    <% @profile.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
</div>
<% end %>
<div class="col-md-5 pull-left">
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name, :placeholder => "Use full name" %>
  </div>
  <div class="field">
    <%= f.label :avatar %><br>
    <%= f.file_field :avatar %>
  </div>
  <div class="field">
    <%= f.label :tagline %><br>
    <%= f.text_field :tagline, :placeholder => "Headline for yourself" %>
  </div>
  <div class="field">
    <%= f.label :website1 %><br>
    <%= f.text_field :website1 %>
  </div>
  <div class="field">
    <%= f.label :website2 %><br>
    <%= f.text_field :website2 %>
  </div>
</div>
<div class="edit_right">

    <%= f.fields_for :art_photo do |ff| %>
      <%= ff.label :art_photo %> 
      <%= ff.file_field :art_photo %>
       <%= ff.label :art_photo %> 
      <%= ff.file_field :art_photo %>
       <%= ff.label :art_photo %> 
      <%= ff.file_field :art_photo %>
    <% end %>
  <div class="field">
    <%= f.label :website3 %><br>
    <%= f.text_field :website3 %>
  </div>

  <div class="field">
    <%= f.label :facebook %><br>
    <%= f.text_field :facebook, :placeholder => "Add facebook url" %>
  </div>
  <div class="field">
    <%= f.label :twitter %><br>
    <%= f.text_field :twitter, :placeholder => "Twitter handle" %>
  </div>
  <div class="field">
    <%= f.label :bio %><br>
    <%= f.text_area :bio, :placeholder => "Brief description" %>
  </div>
  <div class="actions">
    <%= f.submit class: "btn btn-success" %>
  </div>
</div>
<% end %>