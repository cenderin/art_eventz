<!-- primary slider for all events -->
<div class="slider-wrapper theme-default">
  <div class="slider" class="nivoSlider">
    <% @event_photos = EventPhoto.all %>
    <% @event_photos.each do |img| %>
      <%= image_tag(img.photo.url, :title => img.event.title) %>
    <% end %> 
  </div>
</div>


